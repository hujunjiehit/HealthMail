<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/view_bg_card"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/bg_green_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></View>

    <TextView
        android:id="@+id/textView45"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="center_horizontal"
        android:text="@string/burst_price"
        android:textColor="@color/res_white"
        android:textSize="14dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="@+id/view_bg_card"
        app:layout_constraintTop_toTopOf="@+id/view_bg_card" />

    <TextView
        android:id="@+id/tv_force_close_price"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:text="13444.4911111"
        android:textColor="@color/res_white"
        android:textSize="18dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="@+id/view_bg_card"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="@string/risk_rate"
        android:textColor="@color/res_white"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="@+id/view_bg_card"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/textView45" />

    <TextView
        android:id="@+id/tv_risk_rate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="127.23%"
        android:textColor="@color/res_white"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/view_bg_card"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/tv_force_close_price" />


    <View
        android:id="@+id/view01"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:background="@color/res_divider_tile"
        android:layout_marginBottom="20dp"
        app:layout_constraintTop_toBottomOf="@+id/view_bg_card"
        app:layout_constraintBottom_toTopOf="@+id/view_divider">
    </View>

    <TextView
        android:id="@+id/textView50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="@string/history_type"
        android:textColor="@color/res_textColor_3"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/view01"
        app:layout_constraintTop_toTopOf="@+id/view01" />

    <TextView
        android:id="@+id/tv_asset_fenzi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:layout_marginLeft="75dp"
        android:textColor="@color/res_textColor_3"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView50"
        app:layout_constraintTop_toTopOf="@+id/textView50" />

    <TextView
        android:id="@+id/tv_asset_fenmu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="--"
        android:textColor="@color/res_textColor_3"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="@+id/view01"
        app:layout_constraintTop_toTopOf="@+id/textView50" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/available_string"
        android:textColor="@color/res_textColor_2"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView50"
        app:layout_constraintTop_toBottomOf="@+id/textView50" />

    <TextView
        android:id="@+id/tv_fenzi_available"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000000.112113"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/tv_asset_fenzi"
        app:layout_constraintTop_toTopOf="@+id/textView51" />

    <TextView
        android:id="@+id/tv_fenmu_available"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11111.11112113"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_asset_fenmu"
        app:layout_constraintTop_toTopOf="@+id/tv_fenzi_available" />


    <TextView
        android:id="@+id/textView52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21.6dp"
        android:text="@string/unclocked_string"
        android:textColor="@color/res_textColor_2"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView51"
        app:layout_constraintTop_toBottomOf="@+id/textView51" />

    <TextView
        android:id="@+id/tv_fenzi_frozen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000000.112114"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/tv_fenzi_available"
        app:layout_constraintTop_toTopOf="@+id/textView52" />

    <TextView
        android:id="@+id/tv_fenmu_frozen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000000.112113"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_asset_fenmu"
        app:layout_constraintTop_toTopOf="@+id/textView52" />


    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21.6dp"
        android:text="@string/res_has_borrowed"
        android:textColor="@color/res_textColor_2"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView52"
        app:layout_constraintTop_toBottomOf="@+id/textView52" />

    <TextView
        android:id="@+id/tv_fenzi_borrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000000.112114"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/tv_fenzi_frozen"
        app:layout_constraintTop_toTopOf="@+id/textView53" />

    <TextView
        android:id="@+id/tv_fenmu_borrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000000.1121145"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_fenmu_frozen"
        app:layout_constraintTop_toTopOf="@+id/textView53" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21.6dp"
        android:text="@string/res_interest"
        android:textColor="@color/res_textColor_2"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView53"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />


    <TextView
        android:id="@+id/tv_fenzi_interest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000000.1121145"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/tv_fenzi_borrow"
        app:layout_constraintTop_toTopOf="@+id/textView54" />


    <TextView
        android:id="@+id/tv_fenmu_interest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000000.1121146"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_fenmu_borrow"
        app:layout_constraintTop_toTopOf="@+id/textView54" />


    <View
        android:id="@+id/view_divider"
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_marginTop="30dp"
        android:background="@color/res_divider_tile"
        app:layout_constraintTop_toBottomOf="@+id/textView54"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="30dp">
    </View>


    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:text="@string/loan_current"
        android:textColor="@color/res_textColor_1"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_divider" />

    <TextView
        android:id="@+id/tv_history_borrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="22.6dp"
        android:layout_marginEnd="15dp"
        android:gravity="end"
        android:text="@string/loan_history"
        android:textColor="@color/res_blue"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_divider" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:background="@color/res_item_divider"
        app:layout_constraintTop_toBottomOf="@+id/textView55"
        app:layout_constraintStart_toStartOf="parent">
    </View>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />
</androidx.constraintlayout.widget.ConstraintLayout>