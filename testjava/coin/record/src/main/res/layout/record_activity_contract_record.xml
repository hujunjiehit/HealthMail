<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/res_background">

    <include
        layout="@layout/common_layout_top_bar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
    </include>

    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/res_item_divider"
        app:layout_constraintTop_toBottomOf="@id/layout_top_bar"/>

    <TextView
        android:id="@+id/tv_current_order"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:gravity="center_vertical"
        android:text="@string/cur_entrust_label"
        android:textColor="@color/res_textColor_1"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@id/view7"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/history_img14"
        android:layout_width="8dp"
        android:layout_height="13dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@drawable/right_arrow"
        app:layout_constraintBottom_toBottomOf="@+id/tv_current_order"
        app:layout_constraintEnd_toEndOf="@+id/tv_current_order"
        app:layout_constraintTop_toTopOf="@+id/tv_current_order" />


    <View
        android:id="@+id/view8"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginStart="15dp"
        android:background="@color/res_item_divider"
        app:layout_constraintTop_toBottomOf="@id/tv_current_order"/>

    <TextView
        android:id="@+id/tv_history_order"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:gravity="center_vertical"
        android:text="@string/history_entrust_label"
        android:textColor="@color/res_textColor_1"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@id/view8"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/history_img15"
        android:layout_width="8dp"
        android:layout_height="13dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@drawable/right_arrow"
        app:layout_constraintBottom_toBottomOf="@+id/tv_history_order"
        app:layout_constraintEnd_toEndOf="@+id/tv_history_order"
        app:layout_constraintTop_toTopOf="@+id/tv_history_order" />

    <View
        android:id="@+id/view9"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginStart="15dp"
        android:background="@color/res_item_divider"
        app:layout_constraintTop_toBottomOf="@id/tv_history_order"/>

    <TextView
        android:id="@+id/tv_fund_fee"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:gravity="center_vertical"
        android:text="@string/record_fund_cost"
        android:textColor="@color/res_textColor_1"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@id/view9"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/history_img16"
        android:layout_width="8dp"
        android:layout_height="13dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@drawable/right_arrow"
        app:layout_constraintBottom_toBottomOf="@+id/tv_fund_fee"
        app:layout_constraintEnd_toEndOf="@+id/tv_fund_fee"
        app:layout_constraintTop_toTopOf="@+id/tv_fund_fee" />

    <View
        android:id="@+id/view10"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginStart="15dp"
        android:background="@color/res_item_divider"
        app:layout_constraintTop_toBottomOf="@id/tv_fund_fee"/>

</androidx.constraintlayout.widget.ConstraintLayout>