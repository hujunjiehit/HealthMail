<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <TextView
        android:id="@+id/current_delegation_contracts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text=""
        android:textColor="@color/res_textColor_1"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/current_delegation_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:background="@drawable/bg_green_sharp"
        android:paddingLeft="5dp"
        android:paddingTop="1dp"
        android:paddingRight="5dp"
        android:paddingBottom="1dp"
        android:text=""
        android:textColor="@color/res_white"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/current_delegation_contracts"
        app:layout_constraintStart_toEndOf="@+id/current_delegation_contracts"
        app:layout_constraintTop_toTopOf="@+id/current_delegation_contracts" />

    <TextView
        android:id="@+id/tv_type"
        android:layout_width="76dp"
        android:layout_height="14dp"
        android:layout_marginStart="5dp"
        android:textColor="#6b7d8c"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/current_delegation_contracts"
        app:layout_constraintStart_toEndOf="@+id/current_delegation_tag"
        app:layout_constraintTop_toTopOf="@+id/current_delegation_contracts" />


    <TextView
        android:id="@+id/current_delegation_cancel"
        android:layout_width="wrap_content"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:layout_height="20dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/shape_edit_focused_background"
        android:gravity="center"
        android:text="@string/trade_entrust"
        android:textColor="@color/res_blue"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/current_delegation_contracts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/current_delegation_contracts" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="@string/entrust_volume_unit"
        android:textColor="@color/res_textColor_2"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/current_delegation_contracts" />

    <TextView
        android:id="@+id/current_delegation_vol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="4dp"
        android:text="--"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/entrust_price"
        android:textColor="@color/res_textColor_2"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/current_delegation_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/current_delegation_vol"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/current_delegation_vol" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="@string/history_type"
        android:textColor="@color/res_textColor_2"
        android:textSize="11dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView3" />

    <TextView
        android:id="@+id/current_delegation_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/current_delegation_price"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/current_delegation_price" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/deal_unit"
        android:textColor="@color/res_textColor_2"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/current_delegation_vol" />

    <TextView
        android:id="@+id/current_clinch_vol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="--"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/history_item_detail_price"
        android:textColor="@color/res_textColor_2"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <TextView
        android:id="@+id/current_clinch_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/current_clinch_vol"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/current_clinch_vol" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/current_clinch_scale"
        android:textColor="@color/res_textColor_2"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="@+id/current_delegation_type"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/current_clinch_scale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/res_textColor_1"
        android:textSize="13dp"
        android:text="--"
        app:layout_constraintBottom_toBottomOf="@+id/current_clinch_price"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/current_clinch_price" />


    <View
        android:id="@+id/view2"
        android:layout_width="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_height="0.4dp"
        android:layout_marginTop="10dp"
        android:background="@color/res_divider_tile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/current_clinch_price" />

</androidx.constraintlayout.widget.ConstraintLayout>