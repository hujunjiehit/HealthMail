<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/res_blue" />
            <stroke android:width="2px" android:color="@color/res_blue" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <solid android:color="#c5cdd6" />
            <stroke android:width="2px" android:color="#c5cdd6" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/res_blue" />
            <stroke android:width="2px" android:color="@color/res_blue" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>
            <shape>
                <solid android:color="#b33b7bfd" />
                <stroke android:width="2px" android:color="#b33b7bfd" />
            </shape>
        </shape>
    </item>

</selector>